{
  "name": "meliora-impact",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:server",
    "test:server": "cd src/server && npm test",
    "dev": "concurrently npm:dev:client npm:dev:server",
    "dev:client": "cd src/client && npm run dev",
    "dev:server": "cd src/server && npm run dev",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd src/client && npm run build",
    "build:server": "cd src/server && npm run build",
    "postinstall": "npm run install:client && npm run install:server",
    "start": "cd src/server && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/The-13th-reason/Meliora-Impact.git"
  },
  "keywords": [],
